{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "desc": {
      "type": "richtext",
      "required": false
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "img2": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "isNew": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "normal",
        "featured",
        "trending"
      ],
      "default": "normal"
    },
    "catagories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::catagory.catagory",
      "inversedBy": "products"
    },
    "sub_catagories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-catagory.sub-catagory",
      "inversedBy": "products"
    },
    "imgUrl": {
      "type": "string"
    }
  }
}
